package com.simple_calculator;

import jakarta.servlet.RequestDispatcher;
import jakarta.servlet.http.HttpServlet;
import jakarta.servlet.http.HttpServletRequest;
import jakarta.servlet.http.HttpServletResponse;

public class division extends HttpServlet {
	
	public void service(HttpServletRequest req,HttpServletResponse res) {
		
		int n = Integer.parseInt(req.getParameter("nn1"));
		
		int n2 = Integer.parseInt(req.getParameter("nn2"));
		
		int n3 = n/n2;
		
		req.setAttribute("n4",n3);
		
		RequestDispatcher nd = req.getRequestDispatcher("");
		
		
		
	}

}
